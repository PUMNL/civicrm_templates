<?xml version="1.0" encoding="iso-8859-1" ?>

<CaseType>
  <name>Organise Event</name>
  <ActivityTypes>
    <ActivityType>
      <name>Open Case</name>
      <max_instances>1</max_instances>
    </ActivityType>
    <ActivityType>
      <name>Location</name>
    </ActivityType>
    <ActivityType>
      <name>Catering</name>
    </ActivityType>
    <ActivityType>
      <name>Invitation</name>
    </ActivityType>	  
	<ActivityType>
      <name>Equipment</name>
    </ActivityType>
    <ActivityType>
      <name>Presentation</name>
    </ActivityType>
	<ActivityType>
      <name>Reminder</name>
    </ActivityType>
	<ActivityType>
      <name>Nameplates</name>
    </ActivityType>
	<ActivityType>
      <name>Participants List</name>
    </ActivityType>
	<ActivityType>
      <name>Badges</name>
    </ActivityType>
	<ActivityType>
      <name>Number of Participants</name>
    </ActivityType>
	<ActivityType>
      <name>Scenario</name>
    </ActivityType>
	<ActivityType>
      <name>Minutes</name>
    </ActivityType>
	<ActivityType>
      <name>Checklist</name>
    </ActivityType>
	<ActivityType>
      <name>Print PDF Letter</name>
    </ActivityType>
	<ActivityType>
      <name>Email</name>
    </ActivityType>
  </ActivityTypes>
  <ActivitySets>
    <ActivitySet>
      <name>standard_timeline</name>
      <label>Standard timeline</label>
      <timeline>true</timeline>
      <ActivityTypes>
        <ActivityType>
          <name>Open Case</name>
          <status>Completed</status>
        </ActivityType>
	</ActivityTypes>
	</ActivitySet>
    <ActivitySet>
      <name>WOTS</name>
      <label>WOTS</label>
      <timeline>true</timeline>
      <ActivityTypes>
        <ActivityType>
          <name>Invite participants</name>
          <reference_activity>Open Case</reference_activity>
          <reference_offset>3</reference_offset>
          <reference_select>newest</reference_select>
        </ActivityType>
<ActivityType>
          <name>Checkpresentation(s)</name>
          <reference_activity>Open Case</reference_activity>
          <reference_offset>20</reference_offset>
          <reference_select>newest</reference_select>
        </ActivityType>
<ActivityType>
          <name>Instructions ICT</name>
          <reference_activity>Open Case</reference_activity>
          <reference_offset>30</reference_offset>
          <reference_select>newest</reference_select>
        </ActivityType>
      </ActivityTypes>
    </ActivitySet> 
  </ActivitySets>
  <CaseRoles>
    <RelationshipType>
        <name>Event Manager</name>
        <creator>1</creator>
        <manager>1</manager>
    </RelationshipType>
    <RelationshipType>
        <name>Event Project Member</name>
		<manager>2</manager>
    </RelationshipType>
    <RelationshipType>
        <name>Event External Party</name>
    </RelationshipType>
	 <RelationshipType>
        <name>Event Contributor</name>
    </RelationshipType>
 </CaseRoles>
</CaseType>
